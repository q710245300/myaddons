            <field name="trans_level_type_id" ref="TYPE_GB_T2828_1_2012_5"></field>
        </record>
        <record model="qm.trans.level.condition" id="CONDITION_GB_T2828_1_2012_5_2">
            <field name="name">由于发现不符合，当连续抽样计划实施过程中，长时期处在筛选阶段</field>
            <field name="trans_level_type_id" ref="TYPE_GB_T2828_1_2012_5"></field>
        </record>
        <record model="qm.trans.level.condition" id="CONDITION_GB_T2828_1_2012_5_3">
            <field name="name">负责部门同意暂停验收</field>
            <field name="trans_level_type_id" ref="TYPE_GB_T2828_1_2012_5"></field>
        </record>

