            <field name="trans_level_type_id" ref="TYPE_GB_T2828_1_2012_4"></field>
        </record>
        <record model="qm.trans.level.condition" id="CONDITION_GB_T2828_1_2012_4_3">
            <field name="name">于执行5次的样本大小na(T) (参照5.2.1.3)检验过程中，未发现任何之不合格品</field>
            <field name="trans_level_type_id" ref="TYPE_GB_T2828_1_2012_4"></field>
        </record>
        <record model="qm.trans.level.condition" id="CONDITION_GB_T2828_1_2012_4_4">
            <field name="name">产生不合格品之原因已完成矫正</field>
            <field name="trans_level_type_id" ref="TYPE_GB_T2828_1_2012_4"></field>
        </record>
