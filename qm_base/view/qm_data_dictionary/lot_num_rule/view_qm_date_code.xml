<?xml version="1.0"?>
<openerp>
    <data>
        <record id="view_form_data_num" model="ir.ui.view">
            <field name="name">QM Date Num Form</field>
            <field name="model">qm.daytime.num</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group string="请选择">
                            <group name="group_left">
                                <field name="current_time"/>
                            </group>
                            <group name="group_right">
                            </group>
                        </group>
                        <header>
                            <button name="create_date_code" type="object"
                                    string="生成日期编码" class="oe_highlight"/>
                        </header>
                        <group string="">
                            <group name="group_left">
                                <field name="name"/>
                            </group>
                            <group name="group_right">
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <!-- 定义列表视图 -->
        <record id="view_tree_data_num" model="ir.ui.view">
            <field name="name">QM Date Num Tree</field>
            <field name="model">qm.daytime.num</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>


    </data>
</openerp>