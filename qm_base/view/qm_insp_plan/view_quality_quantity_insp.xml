                    <field name="destructive_inspection"></field>
                    <field name="inspection_site"></field>
                    <field name="key_part_nun"></field>
                    <field name="target_value"></field>
                    <field name="lower_bound"></field>
                    <field name="upper_bound"></field>
                    <field name="measurement_units"></field>
                    <field name="inspection_points"></field>
                </tree>
            </field>
